    "webpack-hot-middleware": "^2.18.0"
  },
  "dependencies": {
    "autoprefixer": "^6.7.7",
    "coffeescript": "^1.12.4",
    "css-loader": "^0.28.0",
    "debug": "^2.6.3",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "fuzzy.ai": "^1.0.2",
    "fuzzy.ai-microservice": "^1.1.0",
    "fuzzy.ai-microservice-client": "^1.0.2",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.2",
    "postcss-loader": "^1.3.3",
